{
  "name": "apollo-backend",
  "version": "0.1.0",
  "description": "",
  "main": "./src/server.js",
  "scripts": {
    "start": "bash start.sh",
    "start-docker": "bash start-docker.sh",
    "launch": "nodemon ./src/server.js --exec babel-node -e js",
    "add-component-part": "cd ./src/scripts && bash create-graphql-component.sh",
    "remove-component-part": "cd ./src/scripts && bash delete-graphql-component.sh",
    "build": "babel -d ./build ./src -s",
    "serve-staging": "NODE_ENV=staging node ./server.js",
    "serve-production": "NODE_ENV=production node ./server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "refresh": "cd database && ../node_modules/.bin/prisma deploy",
    "seed": "cd database && ../node_modules/.bin/prisma seed"
  },
  "author": "",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-plugin-root-import": "5.1.0",
    "babel-plugin-transform-async-generator-functions": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-latest": "6.24.1",
    "eslint": "4.18.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-friendly-formatter": "3.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-html": "4.0.2",
    "eslint-plugin-import": "2.9.0",
    "nodemon": "1.18.7"
  },
  "dependencies": {
    "@okgrow/graphql-scalars": "^0.4.2",
    "apollo-server-express": "2.1.0",
    "apollo-server-module-operation-store": "1.3.2",
    "babel-plugin-bulk-import": "^1.0.2",
    "bcrypt": "2.0.0",
    "cookie-parser": "1.4.3",
    "cookie-session": "2.0.0-beta.3",
    "cors": "2.8.4",
    "eslint-plugin-graphql": "^2.1.1",
    "express": "4.16.2",
    "graphql": "^0.13.0",
    "graphql-tools": "4.0.3",
    "jsonwebtoken": "8.1.1",
    "lodash.mapvalues": "4.6.0",
    "merge-graphql-schemas": "^1.5.7",
    "prisma": "^1.22.1",
    "prisma-client-lib": "^1.22.2"
  },
  "license": "MIT"
}
